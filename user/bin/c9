#!/bin/bash

if [[ "$LFS_ENV" != "m2" ]];then
  echo $(basename "$0") "must be run in env_m2"
  exit 1
fi

section="
lfs-bootscripts
"

IFS=$'\n'; for s in $section;do
  frame env_source $s s9_$s
done

frame s9.5.1_eth0_exmaple
frame s9.5.2_resolv.conf
frame s9.5.3_hostname
frame s9.5.4_etc_hosts
frame s9.6.2_sysvinit
frame s9.6.4_clock
frame s9.8_inputrc
frame s9.9_shells
