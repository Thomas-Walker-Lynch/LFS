#!/bin/bash

set -x
# note the capital P 
source package_untar "texinfo"

  sed -e 's/__attribute_nonnull__/__nonnull/' \
      -i gnulib/lib/malloc/dynarray-skeleton.c

  ./configure --prefix=/usr

  # has warnings
  make
  make install

if [[ ! -z "$SOURCE_DIR" ]]; then
    rm -rf $M2_ROOT/source/$SOURCE_DIR
fi
