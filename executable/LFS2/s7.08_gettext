#!/bin/bash
echo \#--------------------------------------------------------------------------------
echo \# $0


set -x
set -e
source package_untar "gettext"

  cd /source/$SOURCE_DIR
  echo "In directory: " $(pwd)

  ./configure --disable-shared
  make
  cp -v gettext-tools/src/{msgfmt,msgmerge,xgettext} /usr/bin

if [[ ! -z "$SOURCE_DIR" ]]; then
    rm -rf $M2_ROOT/source/$SOURCE_DIR
fi
